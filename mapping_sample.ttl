@prefix rr: <http://www.w3.org/ns/r2rml#> .
@prefix rml: <http://semweb.mmlab.be/ns/rml#> .
@prefix ql: <http://semweb.mmlab.be/ns/ql#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rev: <http://purl.org/stuff/rev#> .
@prefix schema: <http://schema.org/> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix base: <http://tib.de/ontario/mapping#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix fnml: <http://semweb.mmlab.be/ns/fnml#> .
@prefix fno: <https://w3id.org/function/ontology#> .
@prefix eablock: <https://github.com/SDM-TIB/EABlock> .
@prefix sdmkgc: <https://github.com/SDM-TIB/KGC/vocab/> .

<disorder_name>
	rml:logicalSource [ rml:source "/mnt/e/GitHub/ESWC-Demo-Dragoman/data-sample.csv";
						rml:referenceFormulation ql:CSV 
						];
	rr:subjectMap <disorder_function_1> ;
    rr:termType rr:IRI;

    rr:predicateObjectMap [
        rr:predicate rdf:type;
        rr:objectMap [ 
            rr:template "https://github.com/SDM-TIB/KGC/vocab/Disorder"; 
        ]
    ].	

<disorder_annotation>
    rml:logicalSource [ rml:source "/mnt/e/GitHub/ESWC-Demo-Dragoman/data-sample.csv";
                        rml:referenceFormulation ql:CSV 
                        ];
    rr:subjectMap [
        rr:template "https://github.com/SDM-TIB/KGC/entity/{ID}";
        rr:class sdmkgc:Disorder
    ];
    rr:predicateObjectMap [
        rr:predicate sdmkgc:hasName;
        rr:objectMap <disorder_function_1> ;
    ].  

##########################################################################################
######################################## Functions #######################################
##########################################################################################

<disorder_function_1>
    fnml:functionValue [
        rml:logicalSource [ rml:source "/mnt/e/GitHub/ESWC-Demo-Dragoman/data-sample.csv";
        rml:referenceFormulation ql:CSV 
                            ];
        rr:predicateObjectMap [
            rr:predicate fno:executes ;
            rr:objectMap [ 
                rr:constant sdmkgc:tolower
            ]
        ];
        rr:predicateObjectMap [
            rr:predicate sdmkgc:value;
            rr:objectMap <disorder_function_2> ;
        ];    
    ].

<disorder_function_2>
    rr:termType rr:IRI;

    fnml:functionValue [
        rml:logicalSource [ rml:source "/mnt/e/GitHub/ESWC-Demo-Dragoman/data-sample.csv";
        rml:referenceFormulation ql:CSV 
                            ];
        rr:predicateObjectMap [
            rr:predicate fno:executes ;
            rr:objectMap [ 
                rr:constant sdmkgc:replaceValue 
            ]
        ];
        rr:predicateObjectMap [
            rr:predicate sdmkgc:value;
            rr:objectMap [
                rr:template "https://github.com/SDM-TIB/KGC/entity/{name}"; 
            ]
        ];
        rr:predicateObjectMap [
            rr:predicate sdmkgc:value2;
            rr:objectMap [ 
                rr:constant " " 
            ]
        ];  
        rr:predicateObjectMap [
            rr:predicate sdmkgc:value3;
            rr:objectMap [ 
                rr:constant "_" 
            ]
        ];   
    ].
